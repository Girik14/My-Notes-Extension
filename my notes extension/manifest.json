{
    "manifest_version": 3,
    "name": "MyNoteExtension",
    "version": "1.0",
    "description": "A simple note-taking extension",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "128": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
